{
  "name": "parlio-frontend",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/parlio-nl/parlio-frontend.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir .",
    "c": "yarn fmt:check && yarn relay:validate && yarn lint && yarn tsc --noEmit && yarn test",
    "relay": "relay-compiler",
    "relay:watch": "relay-compiler --watch",
    "relay:validate": "relay-compiler --validate",
    "fmt": "yarn prettier --write .",
    "fmt:check": "yarn prettier --check .",
    "fmt:ci": "yarn prettier --check --loglevel debug .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "gw:intro": "rover graph introspect http://localhost:4000/graphql > data/schema.graphql"
  },
  "dependencies": {
    "@popperjs/core": "2.11.2",
    "bootstrap": "5.1.3",
    "bootstrap-icons": "1.8.1",
    "graphql": "16.3.0",
    "next": "12.1.0",
    "react": "beta",
    "react-dom": "beta",
    "react-relay": "13.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "13.0.0-alpha.5",
    "@types/bootstrap": "5.1.9",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/react-relay": "13.0.1",
    "@types/relay-runtime": "13.0.1",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-jest-dom": "4.0.1",
    "eslint-plugin-testing-library": "5.0.6",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "relay-compiler": "13.1.1",
    "typescript": "4.5.5"
  }
}
